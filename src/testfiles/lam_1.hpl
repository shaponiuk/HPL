fun (Int () -> Int -> Int ()) sil () = { x ->
        if x () == 0 then 1 else x () * sil (x () - 1)
    };
    
sus fun (() -> Int ()) main () =
    force let
        () :: x = sil ();
        () :: y = sil (6);
        () :: () = print (sil (4));
        () :: () = print (x (5));
        () :: () = print (y ());
