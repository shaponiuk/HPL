struct Main {
    fun (Int () -> Int ()) sil () = { x ->
        if x () == 0 then 1 else x () * sil (x () - 1)
    };
    
    fun (() -> Int ()) main () =
        force let
            () :: x = { e -> sil (e ()) };
            () :: y = { e -> sil (6) };
            () :: () = print (sil (4));
            () :: () = print (x (5));
            () :: () = print (y (1));
        in 0;
};
