struct Main {
    fun (List (Int ()) -> Int ()) sum_list (EmptyListC (())) = 0;
    fun (List (Int ()) -> Int ()) sum_list (ListC (x, l)) = x () + sum_list (l ());
    
    fun (Int ()) main () =
        force let
            Int () :: list = sum_list (ListC (1, EmptyListC (())));
            () :: () = print (list ());
        in 0;
};

data List (a) = ListC ((a (), List (a ()))) | EmptyListC (());